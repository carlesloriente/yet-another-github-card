{
  "name": "yet-another-github-card",
  "version": "1.0.2",
  "description": "Yet another github card profile for use it on your social-networks, blogs or portfolio.",
  "main": "dist/yagc.js",
  "type": "module",
  "module": "dist/yagc.js",
  "files": ["**/*"],
  "homepage": "https://yagc.nocc.dev/",
  "author": "Carles Loriente (https://www.notesoncloudcomputing.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlesloriente/yet-another-github-card.git"
  },
  "bugs": {
    "url": "https://github.com/carlesloriente/yet-another-github-card/issues"
  },
  "funding": [
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/carlesloriente"
    },
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/carlesloriente"
    }
  ],
  "scripts": {
    "lint": "eslint .",
    "build": "rollup -c",
    "test": "jest",
    "build-watch": "npm run build --watch",
    "build-demo": "npm run compile-demo && npm run server-watch",
    "compile-demo": "rollup -c rollup-demo.config.js",
    "server-watch": "NODE_OPTIONS='--trace-deprecation' http-server -c-1 ./demo -o /",
    "docs": "NODE_OPTIONS='--trace-deprecation' http-server -c-1 ./docs -o /"
  },
  "keywords": [
    "github",
    "card",
    "profile",
    "css3",
    "html5",
    "developer"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.1.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0"
  },
  "dependencies": {
    "countup.js": "^2.8.0"
  }
